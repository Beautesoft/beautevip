{
  "name": "kirei",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package",
    "release": "cd ./android && gradlew clean && gradlew assemblerelease",
    "debug": "cd ./android && gradlew clean && gradlew assembledebug %% http-server",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "clean": "cd android && gradlew clean",
    "ipod": "cd ios && pod install",
    "rpod": "cd ios && rm -rf Pods Podfile.lock",
    "rnode": "rm -rf node_modules package-lock.json",
    "rdupres": "rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw",
    "bundleRelease": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && cd android && ./gradlew bundleRelease && cd ..",
    "apkRelease": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/  && cd android && gradlew assembleRelease && cd ..",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && gradlew assembleDebug && cd ../",
    "clear": "cd android && gradlew clean && cd ../",
    "bundleReleaseOnly": "cd android && gradlew clean && gradlew :app:bundleRelease",
    "assembleRelease": "cd android && gradlew clean && gradlew :app:assembleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.11",
